<!DOCTYPE html>
<html>
<head>
    <title>Bargain Books</title>
</head>
<body>
    <h1>Bargain Books</h1>
    <h2>Books under £20</h2>
    <table border="1">
        <tr>
            <th>Book Name</th>
            <th>Price</th>
        </tr>
        <% availableBooks.forEach(function(book) { %>
        <tr>
            <td><%= book.name %></td>
            <td>£<%= book.price %></td>
        </tr>
        <% }); %>
    </table>
    <br>
    <a href="/">Home</a>
</body>
</html>